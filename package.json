{
  "name": "docktor",
  "version": "1.0.2",
  "description": "Surveys Platform, no databases",
  "proxy": "http://localhost:4000",
  "scripts": {
    "start": "react-scripts start",
    "start:api": "NODE_ENV=development node_modules/.bin/nodemon --config nodemon.json api",
    "test:standard": "node_modules/.bin/standard --fix --verbose | snazzy",
    "build": "react-scripts build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gperreymond/docktor.git"
  },
  "keywords": [
    "surveys",
    "platform"
  ],
  "author": {
    "name": "Gilles Perreymond",
    "email": "gperreymond@gmail.com",
    "url": "https://github.com/gperreymond"
  },
  "contributors": {
    "name": "Gilles Perreymond",
    "email": "gperreymond@gmail.com",
    "url": "https://github.com/gperreymond"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gperreymond/docktor/issues"
  },
  "homepage": "https://github.com/gperreymond/docktor#readme",
  "dependencies": {
    "fs-extra": "3.0.1",
    "glob": "7.1.1",
    "hapi": "16.1.1",
    "joi": "10.5.0",
    "lodash": "4.17.4",
    "request": "2.81.0",
    "yamljs": "0.2.10"
  },
  "devDependencies": {
    "debug": "2.6.7",
    "nodemon": "1.11.0",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-cookie": "2.0.7",
    "react-dom": "15.5.4",
    "react-dropzone": "3.13.1",
    "react-markdown": "2.5.0",
    "react-router-dom": "4.1.1",
    "react-scripts": "0.9.5",
    "reflux": "6.4.1",
    "snazzy": "7.0.0",
    "standard": "10.0.2",
    "uuid": "3.0.1"
  },
  "standard": {
    "globals": [
      "Raven",
      "Headers",
      "fetch",
      "before",
      "after",
      "describe",
      "it",
      "expect"
    ],
    "ignore": [
      "build/**",
      "coverage/**",
      "data/**",
      "public/**"
    ]
  }
}
